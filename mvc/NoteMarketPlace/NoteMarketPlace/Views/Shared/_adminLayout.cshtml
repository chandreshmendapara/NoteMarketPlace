                                            <button class="text-left dropdown-item nav-dropdown-item"> LOGOUT</button>
                                        </div>
                                    </div>
                                </li>
                                <!--
    <a class="smooth-scroll" href="#contact-us">Contact Us</a></li>
    <li><a id="user-photo" >
        <img class="img-responsive img-circle" src="img/dashboard/user-img.png" alt="user">
    </a></li>

    -->
                                <li><a href="login.html"><button type="button" class="btn btn-work btn-login smooth-scroll">logout</button></a></li>
