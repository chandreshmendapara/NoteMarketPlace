                                    <a class="dropdown-toggle " data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">Notes</a>
                                    <div class="dropdown-menu note-menu mynav-dropdown-content">
                                        <button class="text-center dropdown-item nav-dropdown-item" onclick="location.href='/admin/noteUnderReview'">Notes Under Review</button>
                                        <button class="text-center dropdown-item nav-dropdown-item" onclick="location.href='/admin/manageAdmin'">Published Notes</button>
