                                <li><a class="smooth-scroll" href="#contact-us">Reports</a></li>
                                <li><a href="sys-config.html">Settings</a></li>
                                <li><a href="javascript:void(0)" onclick="openLogin()" class="btn btn-general btn-login smooth-scroll" title="Login" role="button">Logout</a></li>
                            </ul>
