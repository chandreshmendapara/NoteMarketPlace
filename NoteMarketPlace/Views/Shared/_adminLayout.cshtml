                        <span id="mobile-nav-close-btn">&times;</span>
                        <div id="mobile-nav-content">
                            <ul class="nav">
                                <li><a class="smooth-scroll" href="#search-notes">Dashboard</a></li>
                                <li><a class="smooth-scroll" href="#dashboard">Notes</a></li>
                                <li><a class="smooth-scroll" href="#faq">Members</a></li>
                                <li><a class="smooth-scroll" href="#contact-us">Reports</a></li>
                                <li><a class="smooth-scroll" href="#contact-us">Settings</a></li>
                                <li><a href="javascript:void(0)" onclick="openLogin()" class="btn btn-general btn-login smooth-scroll" title="Login" role="button">Logout</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
